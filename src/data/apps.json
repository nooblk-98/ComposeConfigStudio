{
  "apps": [
    {
      "id": "wordpress",
      "name": "WordPress",
      "version": "latest",
      "description": "Popular CMS platform for websites and blogs",
      "category": "CMS",
      "logo": "https://s.w.org/style/images/about/WordPress-logotype-wmark.png",
      "versions": ["latest", "6.4", "6.3"],
      "defaultPort": 80,
      "image": "wordpress",
      "tools": [
        { "name": "PHP", "version": "8.2" },
        { "name": "Apache", "version": "2.4" }
      ],
      "env": {
        "WORDPRESS_DB_HOST": {
          "key": "WORDPRESS_DB_HOST",
          "value": "db",
          "description": "Database host"
        },
        "WORDPRESS_DB_USER": {
          "key": "WORDPRESS_DB_USER",
          "value": "wordpress",
          "description": "Database user"
        },
        "WORDPRESS_DB_PASSWORD": {
          "key": "WORDPRESS_DB_PASSWORD",
          "value": "wordpress",
          "description": "Database password"
        },
        "WORDPRESS_DB_NAME": {
          "key": "WORDPRESS_DB_NAME",
          "value": "wordpress",
          "description": "Database name"
        }
      },
      "volumes": {
        "data": {
          "path": "/var/www/html",
          "value": "./wordpress",
          "description": "WordPress files and uploads"
        }
      },
      "databases": ["mysql"],
      "features": {
        "runner": false
      }
    },
    {
      "id": "nginx-proxy-manager",
      "name": "Nginx Proxy Manager",
      "version": "latest",
      "description": "Easy-to-use reverse proxy with SSL certificate management",
      "category": "Networking",
      "logo": "https://nginxproxymanager.com/icon.png",
      "versions": ["latest", "2.10", "2.9"],
      "defaultPort": 81,
      "image": "jc21/nginx-proxy-manager",
      "tools": [
        { "name": "Nginx", "version": "latest" },
        { "name": "Let's Encrypt", "version": "integrated" }
      ],
      "env": {},
      "volumes": {
        "data": {
          "path": "/data",
          "description": "Configuration and certificates"
        },
        "letsencrypt": {
          "path": "/etc/letsencrypt",
          "description": "Let's Encrypt certificates"
        }
      },
      "databases": [],
      "features": {
        "runner": false
      }
    },
    {
      "id": "grafana",
      "name": "Grafana",
      "version": "latest",
      "description": "Analytics and monitoring platform with beautiful dashboards",
      "category": "Monitoring",
      "logo": "https://raw.githubusercontent.com/grafana/grafana/main/public/img/grafana_icon.svg",
      "versions": ["latest", "10.2", "10.1"],
      "defaultPort": 3000,
      "image": "grafana/grafana",
      "tools": [
        { "name": "Grafana", "version": "10.x" }
      ],
      "env": {
        "GF_SECURITY_ADMIN_USER": {
          "key": "GF_SECURITY_ADMIN_USER",
          "value": "admin",
          "description": "Admin username"
        },
        "GF_SECURITY_ADMIN_PASSWORD": {
          "key": "GF_SECURITY_ADMIN_PASSWORD",
          "value": "admin",
          "description": "Admin password"
        }
      },
      "volumes": {
        "data": {
          "path": "/var/lib/grafana",
          "description": "Grafana data and dashboards"
        }
      },
      "databases": ["sqlite", "mysql", "postgres"],
      "features": {
        "runner": false
      }
    },
    {
      "id": "plex",
      "name": "Plex Media Server",
      "version": "latest",
      "description": "Stream your media to any device",
      "category": "Media",
      "logo": "https://www.plex.tv/wp-content/themes/plex/assets/img/plex-logo.svg",
      "versions": ["latest"],
      "defaultPort": 32400,
      "image": "plexinc/pms-docker",
      "tools": [
        { "name": "Plex", "version": "latest" }
      ],
      "env": {
        "PLEX_CLAIM": {
          "key": "PLEX_CLAIM",
          "value": "",
          "description": "Plex claim token from plex.tv/claim"
        }
      },
      "volumes": {
        "config": {
          "path": "/config",
          "description": "Plex configuration"
        },
        "media": {
          "path": "/data",
          "description": "Media files"
        }
      },
      "databases": [],
      "features": {
        "runner": false
      }
    },
    {
      "id": "uptime-kuma",
      "name": "Uptime Kuma",
      "version": "latest",
      "description": "Self-hosted monitoring tool like Uptime Robot",
      "category": "Monitoring",
      "logo": "https://uptime.kuma.pet/img/icon.svg",
      "versions": ["latest", "1.23", "1.22"],
      "defaultPort": 3001,
      "image": "louislam/uptime-kuma",
      "tools": [
        { "name": "Node.js", "version": "18" }
      ],
      "env": {},
      "volumes": {
        "data": {
          "path": "/app/data",
          "description": "Uptime Kuma data and configuration"
        }
      },
      "databases": [],
      "features": {
        "runner": false
      }
    },
    {
      "id": "redis",
      "name": "Redis",
      "version": "latest",
      "description": "In-memory data structure store and cache",
      "category": "Database",
      "logo": "https://redis.io/wp-content/uploads/2024/04/Logotype.svg",
      "versions": ["latest", "7.2", "7.0"],
      "defaultPort": 6379,
      "image": "redis",
      "tools": [
        { "name": "Redis", "version": "7.x" }
      ],
      "env": {
        "REDIS_PASSWORD": {
          "key": "REDIS_PASSWORD",
          "value": "",
          "description": "Redis password (optional)"
        }
      },
      "volumes": {
        "data": {
          "path": "/data",
          "description": "Redis data persistence"
        }
      },
      "databases": [],
      "features": {
        "runner": false
      }
    },
    {
      "id": "traefik",
      "name": "Traefik",
      "version": "latest",
      "description": "Modern reverse proxy and load balancer",
      "category": "Networking",
      "logo": "https://doc.traefik.io/traefik/assets/img/traefik.logo.png",
      "versions": ["latest", "v2.10", "v2.9"],
      "defaultPort": 80,
      "image": "traefik",
      "tools": [
        { "name": "Traefik", "version": "2.x" }
      ],
      "env": {},
      "volumes": {
        "docker": {
          "path": "/var/run/docker.sock",
          "description": "Docker socket (required)"
        },
        "config": {
          "path": "/etc/traefik",
          "description": "Traefik configuration"
        }
      },
      "databases": [],
      "features": {
        "runner": false
      }
    }
  ]
}
